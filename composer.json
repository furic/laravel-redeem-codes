    "keywords": [
    	"redeem code",
    	"event",
    	"reward",
        "promo code",
        "coupon code",
        "discount code",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/furic/laravel-redeem-codes",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Richard Fu",
            "email": "fur@richardfu.net"
        }
    ],
    "require": {
    	"php" : "^7.2",
    	"laravel/framework": "~5.8||~6.0||~7.0||~8.0"
    },
    "autoload": {
        "psr-4": {
            "Furic\\RedeemCodes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Furic\\RedeemCodes\\RedeemCodesServiceProvider"
            ]
        }
    }
}